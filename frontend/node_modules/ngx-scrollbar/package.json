      "default": "./fesm2022/ngx-scrollbar-reached-event.mjs"
    },
    "./smooth-scroll": {
      "types": "./smooth-scroll/index.d.ts",
      "esm2022": "./esm2022/smooth-scroll/ngx-scrollbar-smooth-scroll.mjs",
      "esm": "./esm2022/smooth-scroll/ngx-scrollbar-smooth-scroll.mjs",
      "default": "./fesm2022/ngx-scrollbar-smooth-scroll.mjs"
    },
    "./utils": {
      "types": "./utils/index.d.ts",
      "esm2022": "./esm2022/utils/ngx-scrollbar-utils.mjs",
      "esm": "./esm2022/utils/ngx-scrollbar-utils.mjs",
      "default": "./fesm2022/ngx-scrollbar-utils.mjs"
